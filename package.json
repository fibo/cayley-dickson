{
  "name": "cayley-dickson",
  "version": "0.3.0",
  "description": "implements Cayley-Dickson construction to produce a sequence of algebras over a field",
  "main": "cayley-dickson.js",
  "scripts": {
    "check-deps": "npm outdated",
    "lint": "standard",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "postupdate-deps": "npm test && git commit -am ':arrow_up: updated deps' || git checkout -- package.json",
    "test": "NODE_PATH=. tape test.js",
    "update-deps": "npm update --save --dev"
  },
  "pre-commit": [
    "lint",
    "test",
    "check-deps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fibo/cayley-dickson.git"
  },
  "keywords": [
    "algebra",
    "math",
    "real",
    "complex",
    "quaternion",
    "octonion"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/cayley-dickson/issues"
  },
  "homepage": "http://npm.im/cayley-dickson",
  "devDependencies": {
    "pre-commit": "^1.1.2",
    "standard": "^6.0.4",
    "tape": "^4.2.0"
  },
  "dependencies": {
    "algebra-ring": "^0.4.0"
  }
}
